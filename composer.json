{
    "name": "arodu/cakephp-bootstrap-tools",
    "description": "Bootstrap Tools for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0.1",
        "arodu/cakephp-utility-kit": "dev-1.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "BootstrapTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BootstrapTools\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
